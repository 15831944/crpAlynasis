/****************************************************************************************/
/*进度条*/
QProgressBar:horizontal {
border:none;
border-radius:3px;
text-align:center;
background: white;
padding: 1px;
margin-right: 4ex;
}

QProgressBar::chunk {
border-radius:3px;
background: qlineargradient(x1: 0, y1: 0.5, x2: 1, y2: 0.5, stop: 0 green, stop: 1 white);
margin-right: 2px;
width: 10px;
}


