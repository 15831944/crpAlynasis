/*QTabWidget#tabW::pane{border-top:0px solid#e8f3f9;}*/
QTabBar::tab{background: transparent; width:120; height:30; border-bottom-color: #C2C7CB; min-width:100px; min-height:20px; font:18px Times New Roman;}
QTabBar::tab:selected{background: transparent;color: black; background-color:#ffffff;  border-radius:0px;}
QTabBar::tab:hover:selected {background: transparent;color: green; border-color:#ffffff;}
QTabBar::tab:hover:!selected {background: transparent;color: green; border-color:#0E96E4; }
QTabBar::tab:!selected{background: transparent;color: black; background-color:#00C8E5; }


