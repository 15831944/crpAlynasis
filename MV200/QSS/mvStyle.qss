        background-image:url(:/resource/resource/StatusBar/LowWaste_full.png);
        background-repeat:no-repeat; background-position:center;
}
QLabel#LowWaste_normal{ /*低浓度废液-正常*/
        width:35; height:120;
        background-image:url(:/resource/resource/StatusBar/LowWaste_normal.png);
        background-repeat:no-repeat; background-position:center;
}
