/****************************************************************************************/
QWidget:focus{outline: none;} /*去掉选中时的方框*/

/*主界面*/
QWidget#MainWidget{}

/*主界面-状态栏(浮子传感器、温度、试剂状态信息)、功能菜单工具栏（顶部工具栏）*/
QWidget#MainWidgetTop{border-radius:2px;background-color:rgba(50,50,50,50);}

/*主界面-快捷按钮工具栏（左侧工具栏）*/
QWidget#ShortcutBtnBar{border-radius:3px;background-color:rgba(50,50,50,30);}

/*主界面-状态栏*/
QWidget#statusMsgBar{border-radius:2px;background-color:rgba(50,50,50,40);}


/****************************************************************************************/
/**********表头**********/
QHeaderView{
        border: none;
        border-bottom: 3px solid rgb(0, 78, 161);
        background: transparent;
        min-height: 25px;
        font-size:18px;
}
QHeaderView:section:horizontal {
        border: none;
        font-size:18px;
        color: rgb(2, 65, 132);
        background: transparent;
        padding-left: 3px;
}
QHeaderView:section:horizontal:hover {
        color: green;
        font-size:18px;
        background: rgb(0, 78, 161);
}
QHeaderView:section:horizontal:pressed {
        color: black;
        font-size:18px;
        background: rgb(6, 94, 187);
}


/**********表格**********/
/*QTableView样式*/
QTableView,QTableWidget::indicator:checked{
        image:url(://resource/resource/tableView/checked.png);
}

QTableView,QTableWidget::indicator:unchecked{
        image:url(://resource/resource/tableView/unChecked.png);
}

QTableView,QTableWidget {
        border: 1px solid rgb(111, 156, 207);
        gridline-color: rgb(111, 156, 207);
        background: rgb(232, 241, 232);
        font-size:18px;
}
QTableView,QTableWidget:item {
        padding-left: 5px;
        padding-right: 5px;
        border: none;
        font-size:18px;
        background: rgb(232, 241, 252);
        border-right: 1px solid rgb(111, 156, 207);
        border-bottom: 1px solid rgb(111, 156, 207);
}
QTableView,QTableWidget:item:selected {
        background: rgba(252,216,223, 100);
        font-size: 18px;
        color: black;
}
QTableView,QTableWidget#tableView:item:selected:!active {
        font-size:18px;
        color: rgb(65, 65, 65);
}
QTableView,QTableWidget:indicator {
        width: 20px;
        height: 20px;
        font-size:18px;
}
QTableView::indicator:checked{
     image:url(://resource/resource/tableView/checked.png);
}
QTableView::indicator:unchecked{
        image:url(://resource/resource/tableView/unChecked.png);
}

